def get_sum(a,b):
    
    if a == b:
        return a
    
    if a <= b:
        i = list()
        i.append(a)
        while a < b:
            a += 1
            i.append(a)
        x = sum(i)
        return(x)
    
    if a >= b:
        i = list()
        i.append(a)
        while a > b:
            a -= 1
            i.append(a)
        x = sum(i)
        return(x)
