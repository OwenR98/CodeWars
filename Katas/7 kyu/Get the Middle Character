def get_middle(s):
    if len(s) % 2 == 0:
        if len(s) >= 4:
            x = ((len(s) / 2) - 1)
            i = int(x)
            return s[i:-i]
        else:
            return s
    else:
        if len(s) >= 3:
            x = (len(s) / 2)
            i = int(x)
            return s[i:-i]
        else:
            return s
        
