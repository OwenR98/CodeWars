def iq_test(numbers):
    lst = numbers.split()
    
    ls1 = list()
    ls2 = list()
    
    for x,y in enumerate(lst):
        if int(y) % 2 == 0:
            ls1.append(y)
        else:
            ls2.append(y)
            
    if len(ls1) == 1:
        tst = lst.index(ls1[0]) + 1
        return tst
    else:
        tst = lst.index(ls2[0]) + 1
        return tst
