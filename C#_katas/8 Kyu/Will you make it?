// This kata is used to check weather a user can get to a given location on the amount of fuel remaining.

using System;

public static class Kata
{
  public static bool ZeroFuel(uint distanceToPump, uint mpg, uint fuelLeft)
  {
    if (mpg * fuelLeft >= distanceToPump)
    {
      return true;  
    }
    else
    {
      return false;
    }
  }
}
