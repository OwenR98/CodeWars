public class Kata
{
  public static string Bmi(double weight, double height)
  {
    double newBMI = weight / (height * height);
    
    if (newBMI <= 18.5)
    {
      return "Underweight";
    }
    else if (newBMI <= 25.0)
    {
      return "Normal";
    }
    else if (newBMI <= 30.0)
    {
      return "Overweight";
    }
    else
    {
      return "Obese";
    }
  }
}
